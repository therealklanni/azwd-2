{
  "name": "azwebdevs.org",
  "version": "2.0.0",
  "description": "azwebdevs.org",
  "private": true,
  "scripts": {
    "lint": "standard",
    "postinstall": "npm run build && npm prune",
    "prestart": "npm run start:mongo",
    "start": "nodemon -e js,jsx --exec \"npm run build && nf start\"; npm stop",
    "stop": "npm run stop:mongo",
    "start:mongo": "mongod --config mongod.conf --fork",
    "stop:mongo": "killall -9 mongod",
    "build": "babel src -wd build",
    "test": "npm run lint"
  },
  "author": "Kevin Lanni (https://github.com/therealklanni)",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealklanni/azwebdevs.org.git"
  },
  "bugs": {
    "url": "https://github.com/therealklanni/azwebdevs.org/issues"
  },
  "homepage": "https://github.com/therealklanni/azwebdevs.org#readme",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "koa": "^1.1.2",
    "microseconds": "^0.1.0"
  },
  "devDependencies": {
    "foreman": "^1.4.1",
    "nodemon": "^1.8.1",
    "standard": "^6.0.4"
  }
}
