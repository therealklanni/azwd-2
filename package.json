{
  "name": "azwebdevs.org",
  "version": "2.0.0",
  "description": "azwebdevs.org",
  "private": true,
  "scripts": {
    "lint": "eslint . --cache",
    "postinstall": "npm run build && npm prune",
    "prestart": "npm run start:mongo",
    "start": "nodemon --ignore build/ -e js,jsx --exec \"npm run build && nf start\"; npm stop",
    "stop": "npm run stop:mongo",
    "start:mongo": "mongod --config mongod.conf --fork",
    "stop:mongo": "killall -9 mongod && echo Press ENTER to continue",
    "build": "npm run build:clean && npm run build:server && npm run build:common",
    "build:clean": "rimraf build",
    "build:common": "babel --source-root src --source-maps inline -d build/common src/common",
    "build:server": "babel --source-root src --source-maps inline -d build/server src/server",
    "test": "npm run lint"
  },
  "author": "Kevin Lanni (https://github.com/therealklanni)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/therealklanni/azwebdevs.org.git"
  },
  "bugs": {
    "url": "https://github.com/therealklanni/azwebdevs.org/issues"
  },
  "homepage": "https://github.com/therealklanni/azwebdevs.org#readme",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^2.3.0",
    "koa-static": "^2.0.0",
    "microseconds": "^0.1.0",
    "mongoose": "^4.4.3",
    "mongoose-findorcreate": "^0.1.2",
    "rimraf": "^2.5.2",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "eslint-config-troysconfig": "^1.0.8",
    "foreman": "^1.4.1",
    "nodemon": "^1.8.1"
  }
}
